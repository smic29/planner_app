<div class="modal fade" id="loginModal" tabindex="-1" aria-labelledby="loginModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h3 class="modal-title fs-6" id="loginModalLabel">Login or sign up</h3>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body", data-controller="hello">
        <ul class="nav nav-tabs mb-3">
          <li class="nav-item">
            <%= link_to "I'm already a user", new_user_session_path, class:"nav-link active", data: { turbo_frame: :login_signup_frame } %>
          </li>
          <li class="nav-item">
            <%= link_to "Sign me up!", new_user_registration_path, class:"nav-link", data: { turbo_frame: :login_signup_frame }%>
          </li>
        </ul>
        <%= turbo_frame_tag "login_signup_frame", src: new_user_session_path do %>
        <div>
            
            
        </div>
        <% end %>
      </div>
      <%# <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div> %>
    </div>
  </div>
</div>